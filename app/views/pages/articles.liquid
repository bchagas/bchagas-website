{% extends parent %}

{% block 'main' %}
  <h1>{{ page.title }}</h1>

  <ul>
    {% for article in contents.articles %}
      <li>
        <a href='articles/{{ article._permalink ' }}' title='{{ article.title }}'>
          <span>{{ article.tag }}</span> {{ article.title }}
        </a>
      </li>
    {% endfor %}
  </ul>
{% endblock %}

